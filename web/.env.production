# 生产环境配置
# Production Environment Configuration

# API配置 - 生产环境
# 请根据实际部署地址修改
VITE_API_BASE_URL=https://api.campus-lostandfound.com
VITE_API_PREFIX=/api/v1

# WebSocket配置
VITE_WS_URL=wss://api.campus-lostandfound.com/ws/messages

# 应用配置
VITE_APP_TITLE=智能校园失物招领平台
VITE_APP_VERSION=1.0.0

# 高德地图配置 - 生产环境
# 请在高德开放平台申请生产环境Key
VITE_AMAP_KEY=YOUR_PRODUCTION_AMAP_KEY
VITE_AMAP_SECURITY_CODE=YOUR_PRODUCTION_SECURITY_CODE

# 文件上传配置
VITE_MAX_FILE_SIZE=10485760
VITE_MAX_FILES=9

# 分页配置
VITE_DEFAULT_PAGE_SIZE=20

# 调试模式 - 生产环境关闭
VITE_DEBUG=false
